{
  "name": "osho-screen-saver",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "rm -rf build/ public.cache/ && remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "prod": "rm -rf build/ public .cache/ && pnpm build && pnpm start",
    "typecheck": "tsc",
    "update-images": "node update-images.js"
  },
  "dependencies": {
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "isbot": "^4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.5.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
